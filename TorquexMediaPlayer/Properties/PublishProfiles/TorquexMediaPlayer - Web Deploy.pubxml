    <UserName>$TorquexMediaPlayer</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="DefaultConnection" Order="1" Enabled="False">
          <Destination Path="Data Source=tcp:torquexdb1.database.windows.net,1433;Initial Catalog=TorquexPlayer;Persist Security Info=False;User ID=Simon;Password=Jasper123;Connect Timeout=30;Encrypt=True;TrustServerCertificate=False" Name="Server=tcp:torquexdb1.database.windows.net,1433;Initial Catalog=TorquexPlayer;Persist Security Info=False;User ID=Simon;Password=Jasper123;MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;" />
